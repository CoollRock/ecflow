%include <head.h> 
n=1 
while [[ $n -le 5 ]]                # Loop 5 times 
do 
    msg="The date is now $(date)"  
    smslabel info "$msg"            # Set the label 
    sleep 30                        # Wait a 30 seconds 
    (( n = $n + 1 )) 
done 
 
smslabel info "I have now finished my work." 
 
