extern /s4
extern /s4/f1
extern /s4/f2

suite s
 family f1
    autorestore ./f1 ./f2 ./f3 ./f4 # autorestore
 endfamily
 family f2
     autorestore /s/f1 /s/f3 /s/f4 # autorestore
 endfamily
 family f3
